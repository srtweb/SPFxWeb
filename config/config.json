{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "my-mails-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/myMails/MyMailsWebPart.js",
          "manifest": "./src/webparts/myMails/MyMailsWebPart.manifest.json"
        }
      ]
    },
    "create-channels-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/createChannels/CreateChannelsWebPart.js",
          "manifest": "./src/webparts/createChannels/CreateChannelsWebPart.manifest.json"
        }
      ]
    },
    "create-teams-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/createTeams/CreateTeamsWebPart.js",
          "manifest": "./src/webparts/createTeams/CreateTeamsWebPart.manifest.json"
        }
      ]
    },
    "create-channel-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/createChannel/CreateChannelWebPart.js",
          "manifest": "./src/webparts/createChannel/CreateChannelWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "MyMailsWebPartStrings": "lib/webparts/myMails/loc/{locale}.js",
    "CreateChannelsWebPartStrings": "lib/webparts/createChannels/loc/{locale}.js",
    "CreateTeamsWebPartStrings": "lib/webparts/createTeams/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "CreateChannelWebPartStrings": "lib/webparts/createChannel/loc/{locale}.js"
  }
}
